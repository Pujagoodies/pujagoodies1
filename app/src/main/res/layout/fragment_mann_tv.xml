                       android:src="@drawable/live_icon"
                       android:visibility="visible"
                       app:layout_constraintEnd_toEndOf="parent"
                       app:layout_constraintTop_toTopOf="@+id/adminVideo" />
               </androidx.constraintlayout.widget.ConstraintLayout>
